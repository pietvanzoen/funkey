{
  "name": "funkey",
  "version": "0.0.1",
  "description": "A functional keyboard event handler.",
  "main": "dist/funkey.js",
  "config": {
    "srcFiles": [
      "src/key-codes.js",
      "src/funkey.js"
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "posttest": "npm run lint",
    "test": "karma start --single-run",
    "test:watch": "karma start",
    "build": "scripts/build.js",
    "preinstall": "npm prune",
    "preversion": "npm test",
    "version": "npm run build && git add -A",
    "version:patch": "npm version patch -m 'Bump to v%s'",
    "version:minor": "npm version minor -m 'Bump to v%s'",
    "version:major": "npm version major -m 'Bump to v%s'",
    "publish": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pietvanzoen/funkey.git"
  },
  "keywords": [
    "events",
    "keyboard",
    "functional"
  ],
  "author": "Piet van Zoen <hi@pietvanzoen.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pietvanzoen/funkey/issues"
  },
  "homepage": "https://github.com/pietvanzoen/funkey#readme",
  "devDependencies": {
    "eslint": "^3.6.1",
    "eslint-config-google": "^0.6.0",
    "jasmine-core": "^2.5.2",
    "js-beautify": "^1.6.4",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-junit-reporter": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "lodash": "^4.16.3",
    "ramda": "^0.22.1",
    "uglify-js": "^2.7.3"
  }
}
