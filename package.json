{
  "name": "funkey",
  "version": "1.0.3",
  "description": "A functional keyboard event handler.",
  "main": "dist/funkey.js",
  "config": {
    "srcFiles": [
      "src/key-codes.js",
      "src/funkey.js"
    ],
    "testDepFiles": [
      "node_modules/ramda/dist/ramda.js",
      "test/matchers.js"
    ],
    "testFiles": [
      "test/funkey.spec.js"
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "posttest": "yarn run lint",
    "test": "karma start --single-run",
    "test:dist": "TEST_DIST=true npm test",
    "test:watch": "karma start",
    "build": "scripts/build.js",
    "preversion": "yarn run build && yarn run test:dist && git add -A"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pietvanzoen/funkey.git"
  },
  "keywords": [
    "events",
    "keyboard",
    "functional"
  ],
  "author": "Piet van Zoen <hi@pietvanzoen.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pietvanzoen/funkey/issues"
  },
  "homepage": "https://github.com/pietvanzoen/funkey#readme",
  "devDependencies": {
    "eslint": "^3.6.1",
    "eslint-config-google": "^0.7.0",
    "jasmine-core": "^2.5.2",
    "js-beautify": "^1.6.4",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-coveralls": "^1.1.2",
    "karma-firefox-launcher": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-junit-reporter": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "lodash": "^4.16.3",
    "ramda": "^0.22.1",
    "uglify-js": "^2.7.3"
  }
}
